"*":
  "autocomplete-python":
    pythonPaths: "$PROJECT/.venv/bin/python;/usr/local/bin/python"
    useKite: false
  "bracket-matcher": {}
  core:
    disabledPackages: [
      "vim-mode-plus"
      "minimap"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "*.pyc"
      "__pycache__"
    ]
    packagesWithKeymapsDisabled: [
      "git-blame"
      "emmet"
    ]
    projectHome: "/Users/carl/source"
    telemetryConsent: "no"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
  editor:
    fontFamily: "'M+ 1m'"
    invisibles: {}
    tabLength: 4
  emmet: {}
  "exception-reporting":
    userId: "65248596-8647-4142-3b4d-9b0a6a9ca1af"
  "git-blame": {}
  linter:
    lintOnChangeInterval: 500
  "linter-flake8":
    executablePath: "$PROJECT/.venv/bin/flake8;/usr/local/bin/flake8"
    maxLineLength: 120
    projectConfigFile: [
      ".flake8rc"
    ]
  "linter-ui-default": {}
  minimap: {}
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
